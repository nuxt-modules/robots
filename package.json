{
  "name": "nuxt-simple-robots",
  "type": "module",
  "version": "4.0.0-rc.17",
  "packageManager": "pnpm@9.1.4",
  "description": "Tame the robots crawling and indexing your Nuxt site with ease.",
  "author": {
    "name": "Harlan Wilton",
    "email": "harlan@harlanzw.com",
    "url": "https://harlanzw.com/"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://nuxtseo.com/robots",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-simple-robots.git"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/nuxt-simple-robots/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./util": {
      "types": "./dist/runtime/util.d.ts",
      "import": "./dist/runtime/util.js"
    },
    "./dist/runtime/util": {
      "types": "./dist/runtime/util.d.ts",
      "import": "./dist/runtime/util.js"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --fix",
    "client:build": "nuxi generate client",
    "client:dev": "nuxi dev client --port 3300",
    "build": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt-module-build build && npm run client:build",
    "dev": "nuxi dev .playground",
    "dev:build": "nuxi build .playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build build && nuxi prepare .playground",
    "release": "pnpm build && bumpp && pnpm -r publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^1.3.2",
    "@nuxt/kit": "^3.11.2",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "flatted": "^3.3.1",
    "floating-vue": "5.2.2",
    "nuxt-icon": "^0.6.10",
    "nuxt-site-config": "^2.2.12",
    "nuxt-site-config-kit": "^2.2.12",
    "pathe": "^1.1.2",
    "pkg-types": "^1.1.1",
    "shiki": "1.6.1",
    "sirv": "^2.0.4",
    "std-env": "^3.7.0",
    "ufo": "^1.5.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.19.2",
    "@headlessui/vue": "^1.7.22",
    "@iconify-json/carbon": "^1.1.34",
    "@iconify-json/logos": "^1.1.42",
    "@iconify-json/ri": "^1.1.20",
    "@iconify-json/tabler": "^1.1.113",
    "@nuxt/content": "^2.12.1",
    "@nuxt/devtools-ui-kit": "^1.3.2",
    "@nuxt/module-builder": "^0.7.0",
    "@nuxt/test-utils": "3.13.1",
    "@nuxtjs/color-mode": "^3.4.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "8.3.1",
    "@unocss/nuxt": "^0.60.4",
    "@unocss/preset-icons": "^0.60.4",
    "@unocss/preset-uno": "^0.60.4",
    "@unocss/runtime": "^0.60.4",
    "@vueuse/nuxt": "^10.10.0",
    "bumpp": "^9.4.1",
    "eslint": "9.4.0",
    "execa": "^9.1.0",
    "firebase-functions": "^5.0.1",
    "nuxt": "^3.11.2",
    "typescript": "^5.4.5",
    "unocss": "^0.60.4",
    "vitest": "^1.6.0"
  },
  "build": {
    "externals": [
      "h3",
      "consola"
    ]
  },
  "resolutions": {
    "untyped": "1.4.2"
  }
}
