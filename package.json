{
  "name": "@nuxtjs/robots",
  "type": "module",
  "version": "5.6.8",
  "packageManager": "pnpm@10.28.0",
  "description": "Tame the robots crawling and indexing your Nuxt site with ease.",
  "author": {
    "name": "Harlan Wilton",
    "email": "harlan@harlanzw.com",
    "url": "https://harlanzw.com/"
  },
  "contributors": [
    "Ricardo Gobbo de Souza <ricardogobbosouza@yahoo.com.br>",
    "Harlan Wilton <harlan@harlanzw.com>"
  ],
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://nuxtseo.com/robots",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-modules/robots.git"
  },
  "bugs": {
    "url": "https://github.com/nuxt-modules/robots/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./util": "./dist/util.mjs",
    "./content": "./dist/content.mjs"
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      "content": [
        "dist/content"
      ],
      "util": [
        "dist/util"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --fix",
    "client:build": "nuxi generate client",
    "client:dev": "nuxi dev client --port 3300",
    "build": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt-module-build build && npm run client:build",
    "dev": "nuxi dev .playground",
    "prepare:fixtures": "nuxi prepare .playground && nuxi prepare test/fixtures/basic && nuxi prepare test/fixtures/i18n && nuxi prepare test/fixtures/content-v3",
    "dev:build": "nuxi build .playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt-module-build build && nuxi prepare .playground",
    "release": "pnpm build && bumpp -x \"npx changelogen --output=CHANGELOG.md\"",
    "prepack": "pnpm run build",
    "test": "pnpm run prepare:fixtures && vitest",
    "typecheck": "tsc --noEmit",
    "test:attw": "attw --pack"
  },
  "peerDependencies": {
    "zod": ">=3"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "dependencies": {
    "@fingerprintjs/botd": "catalog:",
    "@nuxt/kit": "catalog:",
    "consola": "catalog:",
    "defu": "catalog:",
    "nuxt-site-config": "catalog:",
    "pathe": "catalog:",
    "pkg-types": "catalog:",
    "sirv": "catalog:",
    "std-env": "catalog:",
    "ufo": "catalog:"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:dev",
    "@arethetypeswrong/cli": "catalog:dev",
    "@headlessui/vue": "catalog:dev",
    "@iconify-json/carbon": "catalog:dev",
    "@iconify-json/logos": "catalog:dev",
    "@iconify-json/ri": "catalog:dev",
    "@iconify-json/tabler": "catalog:dev",
    "@nuxt/content": "catalog:dev",
    "@nuxt/devtools-ui-kit": "catalog:dev",
    "@nuxt/module-builder": "catalog:dev",
    "@nuxt/test-utils": "catalog:dev",
    "@nuxtjs/color-mode": "catalog:dev",
    "@nuxtjs/eslint-config-typescript": "catalog:dev",
    "@nuxtjs/i18n": "catalog:dev",
    "@unocss/nuxt": "catalog:dev",
    "@unocss/preset-icons": "catalog:dev",
    "@unocss/preset-uno": "catalog:dev",
    "@unocss/runtime": "catalog:dev",
    "@vueuse/nuxt": "catalog:dev",
    "better-sqlite3": "catalog:dev",
    "bumpp": "catalog:dev",
    "eslint": "catalog:dev",
    "execa": "catalog:dev",
    "firebase-functions": "catalog:dev",
    "happy-dom": "catalog:dev",
    "nuxt": "catalog:dev",
    "playwright-core": "catalog:dev",
    "typescript": "catalog:dev",
    "unocss": "catalog:dev",
    "vitest": "catalog:dev",
    "vue": "catalog:dev",
    "vue-router": "catalog:dev"
  },
  "resolutions": {
    "@nuxtjs/robots": "workspace:*"
  }
}
