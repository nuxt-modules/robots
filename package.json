{
  "name": "@nuxtjs/robots",
  "type": "module",
  "version": "5.6.0",
  "packageManager": "pnpm@10.24.0",
  "description": "Tame the robots crawling and indexing your Nuxt site with ease.",
  "author": {
    "name": "Harlan Wilton",
    "email": "harlan@harlanzw.com",
    "url": "https://harlanzw.com/"
  },
  "contributors": [
    "Ricardo Gobbo de Souza <ricardogobbosouza@yahoo.com.br>",
    "Harlan Wilton <harlan@harlanzw.com>"
  ],
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://nuxtseo.com/robots",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-modules/robots.git"
  },
  "bugs": {
    "url": "https://github.com/nuxt-modules/robots/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./util": "./dist/util.mjs",
    "./content": "./dist/content.mjs"
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      "content": [
        "dist/content"
      ],
      "util": [
        "dist/util"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --fix",
    "client:build": "nuxi generate client",
    "client:dev": "nuxi dev client --port 3300",
    "build": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt-module-build build && npm run client:build",
    "dev": "nuxi dev .playground",
    "prepare:fixtures": "nuxi prepare .playground && nuxi prepare test/fixtures/basic && nuxi prepare test/fixtures/i18n && nuxi prepare test/fixtures/content-v3",
    "dev:build": "nuxi build .playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt-module-build build && nuxi prepare .playground",
    "release": "pnpm build && bumpp -x \"npx changelogen --output=CHANGELOG.md\"",
    "prepack": "pnpm run build",
    "test": "pnpm run prepare:fixtures && vitest",
    "typecheck": "tsc --noEmit",
    "test:attw": "attw --pack"
  },
  "dependencies": {
    "@fingerprintjs/botd": "^2.0.0",
    "@nuxt/kit": "^4.2.1",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "nuxt-site-config": "^3.2.11",
    "pathe": "^2.0.3",
    "pkg-types": "^2.3.0",
    "sirv": "^3.0.2",
    "std-env": "^3.10.0",
    "ufo": "^1.6.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@arethetypeswrong/cli": "^0.18.2",
    "@headlessui/vue": "^1.7.23",
    "@iconify-json/carbon": "^1.2.14",
    "@iconify-json/logos": "^1.2.10",
    "@iconify-json/ri": "^1.2.6",
    "@iconify-json/tabler": "^1.2.23",
    "@nuxt/content": "^3.8.2",
    "@nuxt/devtools-ui-kit": "^3.1.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/test-utils": "^3.20.1",
    "@nuxtjs/color-mode": "^4.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "^10.2.1",
    "@unocss/nuxt": "^66.5.9",
    "@unocss/preset-icons": "^66.5.9",
    "@unocss/preset-uno": "^66.5.9",
    "@unocss/runtime": "^66.5.9",
    "@vueuse/nuxt": "^14.1.0",
    "better-sqlite3": "^12.5.0",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.1",
    "execa": "^9.6.1",
    "firebase-functions": "^7.0.0",
    "happy-dom": "^20.0.11",
    "nuxt": "^4.2.1",
    "playwright-core": "^1.57.0",
    "typescript": "^5.9.3",
    "unocss": "^66.5.9",
    "vitest": "^4.0.14",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@firebase/util",
      "@parcel/watcher",
      "better-sqlite3",
      "esbuild",
      "protobufjs",
      "unrs-resolver",
      "vue-demi"
    ]
  },
  "resolutions": {
    "@nuxtjs/robots": "workspace:*"
  }
}
