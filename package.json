{
  "name": "@nuxtjs/robots",
  "type": "module",
  "version": "5.2.8",
  "packageManager": "pnpm@10.7.1",
  "description": "Tame the robots crawling and indexing your Nuxt site with ease.",
  "author": {
    "name": "Harlan Wilton",
    "email": "harlan@harlanzw.com",
    "url": "https://harlanzw.com/"
  },
  "contributors": [
    "Ricardo Gobbo de Souza <ricardogobbosouza@yahoo.com.br>",
    "Harlan Wilton <harlan@harlanzw.com>"
  ],
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://nuxtseo.com/robots",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-modules/robots.git"
  },
  "bugs": {
    "url": "https://github.com/nuxt-modules/robots/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./util": "./dist/runtime/util.js",
    "./content": "./dist/content.mjs"
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      "content": [
        "dist/content"
      ],
      "util": [
        "dist/util"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --fix",
    "client:build": "nuxi generate client",
    "client:dev": "nuxi dev client --port 3300",
    "build": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt-module-build build && npm run client:build",
    "dev": "nuxi dev .playground",
    "dev:build": "nuxi build .playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt-module-build build && nuxi prepare .playground",
    "release": "pnpm build && bumpp && pnpm -r publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.16.2",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "nuxt-site-config": "^3.1.8",
    "pathe": "^2.0.3",
    "pkg-types": "^2.1.0",
    "sirv": "^3.0.1",
    "std-env": "^3.9.0",
    "ufo": "^1.5.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@headlessui/vue": "^1.7.23",
    "@iconify-json/carbon": "^1.2.8",
    "@iconify-json/logos": "^1.2.4",
    "@iconify-json/ri": "^1.2.5",
    "@iconify-json/tabler": "^1.2.17",
    "@nuxt/content": "^3.4.0",
    "@nuxt/content-v2": "npm:@nuxt/content@2.13.4",
    "@nuxt/devtools-ui-kit": "^2.3.2",
    "@nuxt/module-builder": "^1.0.0",
    "@nuxt/test-utils": "^3.17.2",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "^9.5.2",
    "@unocss/nuxt": "^66.0.0",
    "@unocss/preset-icons": "^66.0.0",
    "@unocss/preset-uno": "^66.0.0",
    "@unocss/runtime": "^66.0.0",
    "@vueuse/nuxt": "^13.0.0",
    "bumpp": "^10.1.0",
    "eslint": "^9.24.0",
    "execa": "^9.5.2",
    "firebase-functions": "^6.3.2",
    "nuxt": "^3.16.2",
    "typescript": "^5.8.3",
    "unocss": "^66.0.0",
    "vitest": "^3.1.1",
    "vue": "3.5.13",
    "vue-router": "^4.5.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "better-sqlite3",
      "esbuild",
      "protobufjs",
      "vue-demi"
    ]
  }
}
